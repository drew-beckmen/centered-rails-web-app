<!DOCTYPE html>
<html>
  <head>
    <title>Centered</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <% if session[:user_id] %>
    <div class="container">
    <%= render "/layouts/header" %>
    </div>
  <% end %>
  <div class="container">
  <body>
    <%= yield %>
  </body>
  </div>
  <%= render "layouts/footer" %>
</html>
